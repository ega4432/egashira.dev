---
import { Image as AstroImage } from "astro:assets";

type Props = {
  alt: string;
  src: string;
  height: number;
  width: number;
  className?: string;
  background?: string;
};

const { alt, src, height, width, className } = Astro.props;
---

<AstroImage
  alt={alt}
  src={src}
  height={height}
  width={width}
  class={`${className || ""}`}
/>
